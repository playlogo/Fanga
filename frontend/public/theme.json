{
    "name": "VSCode Dark Modern",
    "dark": true,
    "author": "Dinesh Srinivasan",
    "editorScheme": "/themes/vscode_dark_brighter.xml",
    "ui": {
        "*": {
            "background": "#181818",
            "foreground": "#DFDFE0",
            "infoForeground": "#A9A9AA",
            "selectionInactiveBackground": "#37373d",

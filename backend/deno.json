{
    "tasks": {
        "dev": "deno --watch --allow-write --allow-read --allow-env --allow-net src/main.ts",
        "prod": "deno --allow-write --allow-read --allow-env --allow-net src/main.ts",
        "cache": "deno cache src/main.ts",
        "lint": "deno lint",
        "check": "deno check src/main.ts src/**/*.ts --allow-import"
    },
    "lint": {
        "rules": {
            "exclude": [
                "no-explicit-any"
            ]
        }
    }
}